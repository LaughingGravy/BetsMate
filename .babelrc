{
    "presets": ["env","react","stage-2"],
    "plugins": ["transform-runtime"],
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        },
        "production": {
            "plugins": [
                "@babel/plugin-transform-react-constant-elements", 
                "transform-react-remove-prop-types", {
                    "mode": "wrap",
                    "ignoreFilenames": ["node_modules"]
                }
            ]
        }
    }
}