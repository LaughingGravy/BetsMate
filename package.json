{
  "name": "Betsmate",
  "version": "1.0.0",
  "description": "Betsmate Football Predictor Application",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "//": "webpack-dev-server --open --mode development --config webpack/webpack.dev.config.js",
    "/start/": "cross-env NODE_ENV=development RUN_ENGINE=false node src/server/index.js --mode development",
    "oldserver": "cross-env NODE_ENV=production RUN_ENGINE=true node src/server/index.js --mode production",
    "oldbuild": "cross-env NODE_ENV=production' webpack --mode production --config webpack/webpack.prod.config.js",
    "clean": "rimraf dist",
    "build-run": "npm run build && npm run server",
    "server-dev": "node dist/server-dev",
    "start": "run-p browser server-dev",
    "server": "node dist/server",
    "build-browser-dev": "cross-env NODE_ENV=development WEBPACK_CONFIG=browser-dev BROWSER=true webpack --mode development --colors",
    "build-server-dev": "cross-env NODE_ENV=development WEBPACK_CONFIG=server-dev SERVER=true webpack --mode development --colors",
    "build-dev": "npm run build-browser-dev && npm run build-server-dev",
    "build-browser-prod": "cross-env WEBPACK_CONFIG=browser-prod webpack",
    "build-server-prod": "cross-env WEBPACK_CONFIG=server-prod webpack",
    "build-prod": "npm run build-browser-prod && npm run build-server-prod"
  },
